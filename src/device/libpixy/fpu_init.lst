Nvoid fpuInit(void)
N{
N// from arm trm manual:
N//                ; CPACR is located at address 0xE000ED88
N//                LDR.W R0, =0xE000ED88
N//                ; Read CPACR
N//                LDR R1, [R0]
N//                ; Set bits 20-23 to enable CP10 and CP11 coprocessors
N//                ORR R1, R1, #(0xF << 20)
N//                ; Write back the modified value to the CPACR
N//                STR R1, [R0]
N
N                
N    volatile uint32_t* regCpacr = (uint32_t*) LPC_CPACR;
X    volatile uint32_t* regCpacr = (uint32_t*) 0xE000ED88;
N    volatile uint32_t* regMvfr0 = (uint32_t*) SCB_MVFR0;
X    volatile uint32_t* regMvfr0 = (uint32_t*) 0xE000EF40;
N    volatile uint32_t* regMvfr1 = (uint32_t*) SCB_MVFR1;
X    volatile uint32_t* regMvfr1 = (uint32_t*) 0xE000EF44;
N    volatile uint32_t Cpacr;
N    volatile uint32_t Mvfr0;
N    volatile uint32_t Mvfr1;    
N    char vfpPresent = 0;
N
N    Mvfr0 = *regMvfr0;
N    Mvfr1 = *regMvfr1;
N
N    vfpPresent = ((SCB_MVFR0_RESET == Mvfr0) && (SCB_MVFR1_RESET == Mvfr1));
X    vfpPresent = ((0x10110021 == Mvfr0) && (0x11000011 == Mvfr1));
N    
N    if(vfpPresent)
N    {
N        Cpacr = *regCpacr;
N        Cpacr |= (0xF << 20);
N        *regCpacr = Cpacr;   // enable CP10 and CP11 for full access
N    }
N
N}
N
