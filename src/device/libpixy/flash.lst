N#define FLASH_SECTOR_SIZE        	0x1000
N#define FLASH_SECTOR_MASK(a)	 	(a&(~(FLASH_SECTOR_SIZE-1)))
N#define FLASH_SIZE               	g_spifi.memSize
N#define FLASH_BEGIN	             	0x14000000
N#define FLASH_BEGIN_B	 			g_spifi.base
N#define FLASH_END		 			(FLASH_BEGIN+FLASH_SIZE)
N#define FLASH_END_B		 			(FLASH_BEGIN_B+FLASH_SIZE)
N#define FLASH_OFFSET     			(FLASH_BEGIN_B-FLASH_BEGIN)
N
Nint32_t flash_erase(uint32_t addr, uint32_t len);
Nint32_t flash_program(uint32_t addr, const uint8_t *data, uint32_t len);
N
N#endif
