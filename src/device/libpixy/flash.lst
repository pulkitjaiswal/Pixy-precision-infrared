N#include "spifi_rom_api.h"
N#define CSHIGH 4
N#define SPIFI_MHZ 80
N#define source_data_ad (char *)1234
N
N	int32_t rc;
N	SPIFIopers opers;
N
N	define_spifi_romPtr(spifi);
N	SPIFIobj *obj = malloc(sizeof(SPIFIobj));
N	if (!obj) { can't allocate memory }
N
N	rc = spifi->spifi_init (obj, CSHIGH, S_FULLCLK+S_RCVCLK, SPIFI_MHZ);
N	if (rc) { investigate init error rc }
N	printf ("the serial flash contains %d bytes\n", obj->devSize);
N
N	opers.dest = where_to_program;
N	opers.length = how_many_bytes;
N	opers.scratch = NULL;			// unprogrammed data is not saved/restored
N	opers.protect = -1;				// save & restore protection
N	opers.options = S_VERIFY_PROG;
N
N	rc = spifi->spifi_program (obj, source_data_ad, &opers);
N	if (rc) { investigate program error rc }
N*/
