L 18 "flash.cpp" 2
N
Nint32_t flash_erase(uint32_t addr, uint32_t len)
N{
N	SPIFIopers spifi;
N	uint32_t addrTemp;
N	uint32_t i;
N
N	// add offset
N	if (addr>=FLASH_BEGIN && addr<=FLASH_END)
X	if (addr>=0x14000000 && addr<=(0x14000000+g_spifi . memSize))
N		addr += FLASH_OFFSET;
X		addr += (g_spifi . base -0x14000000);
N
N	memset((void *)&spifi, 0, sizeof(spifi));
N
N	for (i=0; i<len; i+=FLASH_SECTOR_SIZE)
X	for (i=0; i<len; i+=0x1000)
N	{ 
N		addrTemp = FLASH_SECTOR_MASK(addr+i);
X		addrTemp = (addr+i&(~(0x1000-1)));
N		spifi.dest = (char *)addrTemp;
N		spifi.length = FLASH_SECTOR_SIZE;
X		spifi.length = 0x1000;
N		spifi.scratch = NULL;
X		spifi.scratch = 0;
N		spifi.options = S_VERIFY_ERASE;
X		spifi.options = 0x20;
N		if (spifi_erase(&g_spifi, &spifi)) 
N			return -1;
N	}
N	return 0;
N}
N
Nint32_t flash_program(uint32_t addr, const uint8_t *data, uint32_t len)
N{
N	SPIFIopers spifi;
N	// add offset
N	if (addr>=FLASH_BEGIN && addr<=FLASH_END)
X	if (addr>=0x14000000 && addr<=(0x14000000+g_spifi . memSize))
N		addr += FLASH_OFFSET;
X		addr += (g_spifi . base -0x14000000);
N
N 	memset((void *)&spifi, 0, sizeof(spifi));
N
N	spifi.dest = (char *)addr;
N	spifi.length = len;
N	spifi.scratch = NULL;
X	spifi.scratch = 0;
N	spifi.options = S_VERIFY_PROG;
X	spifi.options = 0x10;
N	if (spifi_program(&g_spifi, (char *)data, &spifi))
N		return -1;
N	return 0;
N}
